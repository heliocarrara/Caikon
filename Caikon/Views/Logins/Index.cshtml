@model IEnumerable<Caikon.Models.Login>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Acesso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Senha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Validade)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Acesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Senha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Validade)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LoginUID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LoginUID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LoginUID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
